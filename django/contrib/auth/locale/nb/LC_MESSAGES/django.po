msgstr "hash"

msgid "variety"
msgstr "variasjon"

msgid "version"
msgstr "versjon"

msgid "memory cost"
msgstr "minnekostnad"

msgid "time cost"
msgstr "tidskostnad"

msgid "parallelism"
msgstr "parallellitet"

msgid "work factor"
msgstr "arbeidsfaktor"

msgid "checksum"
msgstr "sjekksum"

msgid "name"
msgstr "navn"

msgid "content type"
msgstr "innholdstype"

msgid "codename"
msgstr "kodenavn"

