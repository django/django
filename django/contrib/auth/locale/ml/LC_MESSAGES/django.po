msgstr "പ്രധാന തീയതികള്‍"

#: admin.py:31
msgid "Groups"
msgstr "ഗ്രൂപ്പുകള്‍"

#: admin.py:113
msgid "Password changed successfully."
msgstr "പാസ് വേര്‍ഡ് മാറ്റിയിരിക്കുന്നു."

#: admin.py:123
#, python-format
