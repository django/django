msgstr "درست صحیح عدد درج کریں۔"

msgid "Enter a valid email address."
msgstr "درست ای میل پتہ درج کریں۔"

#. Translators: "letters" means latin letters: a-z and A-Z.
msgid ""
"Enter a valid “slug” consisting of letters, numbers, underscores or hyphens."
msgstr "درست “slug” درج کریں جو حروف، اعداد، انڈر اسکور یا ہائفن پر مشتمل ہو۔"

msgid ""
"Enter a valid “slug” consisting of Unicode letters, numbers, underscores, or "
"hyphens."
msgstr "درست “slug” درج کریں جو یونیکوڈ حروف، اعداد، انڈر اسکور یا ہائفن پر مشتمل ہو۔"

#, python-format
msgid "Enter a valid %(protocol)s address."
msgstr "درست %(protocol)s پتہ درج کریں۔"

msgid "IPv4"
msgstr "IPv4"

msgid "IPv6"
msgstr "IPv6"

msgid "IPv4 or IPv6"
msgstr "IPv4 یا IPv6"

msgid "Enter only digits separated by commas."
msgstr "صرف اعداد درج کریں جو کوموں سے الگ کئے ھوئے ھوں۔"

#, python-format
msgid "Ensure this value is %(limit_value)s (it is %(show_value)s)."
msgstr ""
"اس بات کا یقین کر لیں کہ یہ قیمت (ویلیو) %(limit_value)s ہے (یہ %(show_value)s ہے)۔"

#, python-format
msgid "Ensure this value is less than or equal to %(limit_value)s."
msgstr ""
"اس بات کا یقین کر لیں کہ یہ قیمت (ویلیو) %(limit_value)s سے کم یا اس کے "
"برابر ھے۔"

#, python-format
msgid "Ensure this value is greater than or equal to %(limit_value)s."
msgstr ""
"اس بات کا یقین کر لیں کہ یہ قیمت (ویلیو) %(limit_value)s سے زیادہ یا اس کے "
"برابر ھے۔"

#, python-format
msgid "Ensure this value is a multiple of step size %(limit_value)s."
msgstr "اس بات کا یقین کر لیں کہ یہ قدر قدم کے سائز %(limit_value)s کی مضاعف ہے۔"

#, python-format
msgid ""
"Ensure this value is a multiple of step size %(limit_value)s, starting from "
"%(offset)s, e.g. %(offset)s, %(valid_value1)s, %(valid_value2)s, and so on."
msgstr ""
"اس بات کا یقین کر لیں کہ یہ قدر قدم کے سائز %(limit_value)s کی مضاعف ہے، "
"%(offset)s سے شروع کرتے ہوئے، مثلاً %(offset)s، %(valid_value1)s، "
"%(valid_value2)s، وغیرہ۔"

#, python-format
msgid ""
"Ensure this value has at least %(limit_value)d character (it has "
"%(show_value)d)."
msgid_plural ""
"Ensure this value has at least %(limit_value)d characters (it has "
"%(show_value)d)."
msgstr[0] "اس بات کا یقین کر لیں کہ اس قدر میں کم از کم %(limit_value)d حرف ہو (اس میں %(show_value)d ہے)۔"
msgstr[1] "اس بات کا یقین کر لیں کہ اس قدر میں کم از کم %(limit_value)d حروف ہوں (اس میں %(show_value)d ہیں)۔"

#, python-format
msgid ""
"Ensure this value has at most %(limit_value)d character (it has "
"%(show_value)d)."
msgid_plural ""
"Ensure this value has at most %(limit_value)d characters (it has "
"%(show_value)d)."
msgstr[0] "اس بات کا یقین کر لیں کہ اس قدر میں زیادہ سے زیادہ %(limit_value)d حرف ہو (اس میں %(show_value)d ہے)۔"
msgstr[1] "اس بات کا یقین کر لیں کہ اس قدر میں زیادہ سے زیادہ %(limit_value)d حروف ہوں (اس میں %(show_value)d ہیں)۔"

msgid "Enter a number."
msgstr "نمبر درج کریں۔"

#, python-format
msgid "Ensure that there are no more than %(max)s digit in total."
msgid_plural "Ensure that there are no more than %(max)s digits in total."
msgstr[0] "اس بات کا یقین کر لیں کہ مجموعی طور پر %(max)s سے زیادہ ہندسہ نہ ہو۔"
msgstr[1] "اس بات کا یقین کر لیں کہ مجموعی طور پر %(max)s سے زیادہ ہندسے نہ ہوں۔"

#, python-format
msgid "Ensure that there are no more than %(max)s decimal place."
msgid_plural "Ensure that there are no more than %(max)s decimal places."
msgstr[0] "اس بات کا یقین کر لیں کہ %(max)s سے زیادہ اعشاری جگہ نہ ہو۔"
msgstr[1] "اس بات کا یقین کر لیں کہ %(max)s سے زیادہ اعشاری جگہیں نہ ہوں۔"

#, python-format
msgid ""
"Ensure that there are no more than %(max)s digit before the decimal point."
msgid_plural ""
"Ensure that there are no more than %(max)s digits before the decimal point."
msgstr[0] "اس بات کا یقین کر لیں کہ اعشاری نقطے سے پہلے %(max)s سے زیادہ ہندسہ نہ ہو۔"
msgstr[1] "اس بات کا یقین کر لیں کہ اعشاری نقطے سے پہلے %(max)s سے زیادہ ہندسے نہ ہوں۔"

#, python-format
msgid ""
"File extension “%(extension)s” is not allowed. Allowed extensions are: "
"%(allowed_extensions)s."
msgstr ""
"فائل کی توسیع “%(extension)s” کی اجازت نہیں ہے۔ اجازت یافتہ توسیعات: "
"%(allowed_extensions)s۔"

msgid "Null characters are not allowed."
msgstr "نل حروف کی اجازت نہیں ہے۔"

msgid "and"
msgstr "اور"

#, python-format
msgid "%(model_name)s with this %(field_labels)s already exists."
msgstr "%(field_labels)s کے ساتھ %(model_name)s پہلے ہی موجود ہے۔"

#, python-format
msgid "Constraint “%(name)s” is violated."
msgstr "قید “%(name)s” کی خلاف ورزی ہوئی ہے۔"

#, python-format
msgid "Value %(value)r is not a valid choice."
msgstr "قدر %(value)r درست انتخاب نہیں ہے۔"

msgid "This field cannot be null."
msgstr "یہ خانہ نامعلوم (null( نھیں رہ سکتا۔"

msgid "This field cannot be blank."
msgstr "یہ خانہ خالی نھیں چھوڑا جا سکتا۔"

#, python-format
msgid "%(model_name)s with this %(field_label)s already exists."
msgstr "%(model_name)s اس %(field_label)s کے ساتھ پہلے ہی موجود ھے۔"

#. Translators: The 'lookup_type' is one of 'date', 'year' or
#. 'month'. Eg: "Title must be unique for pub_date year"
#, python-format
msgid ""
"%(field_label)s must be unique for %(date_field_label)s %(lookup_type)s."
msgstr "%(field_label)s کو %(date_field_label)s کے %(lookup_type)s کے لیے منفرد ہونا چاہیے۔"

#, python-format
msgid "Field of type: %(field_type)s"
msgstr "%(field_type)s قسم کا خانہ"

#, python-format
msgid "“%(value)s” value must be either True or False."
msgstr "“%(value)s” کی قدر True یا False میں سے ایک ہونی چاہیے۔"

#, python-format
msgid "“%(value)s” value must be either True, False, or None."
msgstr "“%(value)s” کی قدر True، False یا None میں سے ایک ہونی چاہیے۔"

msgid "Boolean (Either True or False)"
msgstr "بولین (True یا False)"

#, python-format
msgid "String (up to %(max_length)s)"
msgstr "سلسلۂ حروف (String) (%(max_length)s تک)"

msgid "String (unlimited)"
msgstr "سلسلۂ حروف (String) (لامحدود)"

msgid "Comma-separated integers"
msgstr " کومے سے الگ کئے ھوئے صحیح اعداد"

#, python-format
msgid ""
"“%(value)s” value has an invalid date format. It must be in YYYY-MM-DD "
"format."
msgstr ""
"“%(value)s” کی قدر کی تاریخ کی ساخت درست نہیں ہے۔ یہ YYYY-MM-DD فارمیٹ "
"میں ہونی چاہیے۔"

#, python-format
msgid ""
"“%(value)s” value has the correct format (YYYY-MM-DD) but it is an invalid "
"date."
msgstr ""
"“%(value)s” کی قدر درست فارمیٹ (YYYY-MM-DD) میں ہے لیکن تاریخ درست نہیں ہے۔"

msgid "Date (without time)"
msgstr "تاریخ (وقت کے بغیر)"

#, python-format
msgid ""
"“%(value)s” value has an invalid format. It must be in YYYY-MM-DD HH:MM[:ss[."
"uuuuuu]][TZ] format."
msgstr ""
"“%(value)s” کی قدر کی ساخت درست نہیں ہے۔ یہ YYYY-MM-DD HH:MM[:ss[.uuuuuu]]"
"[TZ] فارمیٹ میں ہونی چاہیے۔"

#, python-format
msgid ""
"“%(value)s” value has the correct format (YYYY-MM-DD HH:MM[:ss[.uuuuuu]]"
"[TZ]) but it is an invalid date/time."
msgstr ""
"“%(value)s” کی قدر درست فارمیٹ (YYYY-MM-DD HH:MM[:ss[.uuuuuu]][TZ]) میں ہے "
"لیکن تاریخ/وقت درست نہیں ہے۔"

msgid "Date (with time)"
msgstr "تاریخ (بمع وقت)"

#, python-format
msgid "“%(value)s” value must be a decimal number."
msgstr "“%(value)s” کی قدر اعشاری عدد ہونی چاہیے۔"

